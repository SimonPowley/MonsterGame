      <TextField fx:id="changeNameField" layoutX="225.0" layoutY="206.0" onAction="#onNameEntered" promptText="Change Monster Name" />
      <Label fx:id="gameLogLabel" alignment="TOP_CENTER" layoutX="195.0" layoutY="16.0" prefHeight="97.0" prefWidth="210.0" text="Game Log Label">
