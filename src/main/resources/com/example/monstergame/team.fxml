                        <Label fx:id="team1NameLabel" prefHeight="17.0" prefWidth="121.0" text="Slot Empty" />
                        <Label fx:id="team1LevelLabel" prefHeight="17.0" prefWidth="227.0" text="Lvl: 5" />
