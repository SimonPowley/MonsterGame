<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #36454F;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.monstergame.TitleController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="250.0" layoutY="136.0" prefHeight="200.0" prefWidth="100.0" spacing="5.0">
         <children>
            <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#onNewGame" prefHeight="25.0" prefWidth="85.0" text="New Game" />
            <Button fx:id="loadGameButton" mnemonicParsing="false" onAction="#onLoadGame" prefHeight="25.0" prefWidth="85.0" text="Load Game" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onExit" prefHeight="25.0" prefWidth="85.0" text="Exit Game" />
         </children>
      </VBox>
   </children>
</AnchorPane>
