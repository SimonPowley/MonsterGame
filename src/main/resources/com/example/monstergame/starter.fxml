<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.monstergame.StarterController">
   <Label fx:id="gameLogLabel" alignment="TOP_CENTER" layoutX="21.0" layoutY="295.0" prefHeight="53.0" prefWidth="557.0"
          text="Choose a Monster" textFill="#313030">
      <font>
         <Font name="Franklin Gothic Book Italic" size="24.0"/>
      </font>
   </Label>
   <TextField fx:id="monsterNameTextField" alignment="CENTER" layoutX="225.0" layoutY="261.0"
              onAction="#onMonsterChosen" promptText="Enter monster name"/>
   <Button fx:id="rerollButton" layoutX="250.0" layoutY="359.0" mnemonicParsing="false" onMouseClicked="#onRerollButton"
           text="Reroll Monsters"/>
   <HBox alignment="CENTER" layoutX="85.0" layoutY="14.0" prefHeight="239.0" prefWidth="430.0" spacing="50.0">
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="110.0">
         <Circle fx:id="fireMonsterCircle" fill="#ff2424" onMouseClicked="#onFireMonsterChosen" radius="31.0"
                 stroke="BLACK" strokeType="INSIDE" strokeWidth="2.0"/>
         <Label fx:id="fireNameLabel" alignment="TOP_LEFT" prefHeight="0.0" prefWidth="110.0" text="Fire Monster">
            <font>
               <Font name="System Bold" size="14.0"/>
            </font>
         </Label>
         <HBox prefHeight="17.0" prefWidth="110.0">
            <VBox prefHeight="89.0" prefWidth="55.0">
               <Label fx:id="fireHpLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="HP: XX/XX"/>
               <Label fx:id="fireAttackLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Att: XX"/>
               <Label fx:id="fireDefenseLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Def: XX"/>
               <Label fx:id="fireSpeedLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Spe: XX"/>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="55.0">
               <Label fx:id="fireHpBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="+10%"/>
               <Label fx:id="fireAttackBoostLabel" alignment="TOP_CENTER" prefHeight="17.0" prefWidth="55.0"
                      text="-20%"/>
               <Label fx:id="fireDefenseBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="+15%"/>
               <Label fx:id="fireSpeedBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="-5%"/>
            </VBox>
         </HBox>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <Label prefHeight="17.0" prefWidth="223.0" text="Moves">
               <font>
                  <Font name="System Bold" size="12.0"/>
               </font>
            </Label>
            <Label fx:id="fireMove1Label" prefHeight="17.0" prefWidth="231.0" text="Move 1"/>
            <Label fx:id="fireMove2Label" prefHeight="17.0" prefWidth="211.0" text="Move 2"/>
            <Label fx:id="fireMove3Label" prefHeight="17.0" prefWidth="233.0" text="Move 3"/>
            <Label fx:id="fireMove4Label" prefHeight="17.0" prefWidth="267.0" text="Move 4"/>
         </VBox>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="110.0">
         <Circle fx:id="waterMonsterCircle" fill="#5252ff" onMouseClicked="#onWaterMonsterChosen" radius="31.0"
                 stroke="BLACK" strokeType="INSIDE" strokeWidth="2.0"/>
         <Label fx:id="waterNameLabel" alignment="TOP_LEFT" prefHeight="0.0" prefWidth="110.0"
                text="Water Monster">
            <font>
               <Font name="System Bold" size="14.0"/>
            </font>
         </Label>
         <HBox prefHeight="17.0" prefWidth="110.0">
            <VBox prefHeight="89.0" prefWidth="55.0">
               <Label fx:id="waterHpLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="HP: XX/XX"/>
               <Label fx:id="waterAttackLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Att: XX"/>
               <Label fx:id="waterDefenseLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Def: XX"/>
               <Label fx:id="waterSpeedLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Spe: XX"/>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="55.0">
               <Label fx:id="waterHpBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="+10%"/>
               <Label fx:id="waterAttackBoostLabel" alignment="TOP_CENTER" prefHeight="17.0"
                      prefWidth="55.0" text="-20%"/>
               <Label fx:id="waterDefenseBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="+15%"/>
               <Label fx:id="waterSpeedBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="-5%"/>
            </VBox>
         </HBox>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <Label prefHeight="17.0" prefWidth="223.0" text="Moves">
               <font>
                  <Font name="System Bold" size="12.0"/>
               </font>
            </Label>
            <Label fx:id="waterMove1Label" prefHeight="17.0" prefWidth="231.0" text="Move 1"/>
            <Label fx:id="waterMove2Label" prefHeight="17.0" prefWidth="211.0" text="Move 2"/>
            <Label fx:id="waterMove3Label" prefHeight="17.0" prefWidth="233.0" text="Move 3"/>
            <Label fx:id="waterMove4Label" prefHeight="17.0" prefWidth="267.0" text="Move 4"/>
         </VBox>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="110.0">
         <Circle fx:id="natureMonsterCircle" fill="#00a800" onMouseClicked="#onNatureMonsterChosen" radius="31.0"
                 stroke="BLACK" strokeType="INSIDE" strokeWidth="2.0"/>
         <Label fx:id="natureNameLabel" alignment="TOP_LEFT" prefHeight="0.0" prefWidth="110.0"
                text="Nature Monster">
            <font>
               <Font name="System Bold" size="14.0"/>
            </font>
         </Label>
         <HBox prefHeight="17.0" prefWidth="110.0">
            <VBox prefHeight="89.0" prefWidth="55.0">
               <Label fx:id="natureHpLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="HP: XX/XX"/>
               <Label fx:id="natureAttackLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Att: XX"/>
               <Label fx:id="natureDefenseLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Def: XX"/>
               <Label fx:id="natureSpeedLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="55.0"
                      text="Spe: XX"/>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="55.0">
               <Label fx:id="natureHpBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="+10%"/>
               <Label fx:id="natureAttackBoostLabel" alignment="TOP_CENTER" prefHeight="17.0"
                      prefWidth="55.0" text="-20%"/>
               <Label fx:id="natureDefenseBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="+15%"/>
               <Label fx:id="natureSpeedBoostLabel" alignment="TOP_CENTER" prefWidth="55.0" text="-5%"/>
            </VBox>
         </HBox>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <Label prefHeight="17.0" prefWidth="223.0" text="Moves">
               <font>
                  <Font name="System Bold" size="12.0"/>
               </font>
            </Label>
            <Label fx:id="natureMove1Label" prefHeight="17.0" prefWidth="231.0" text="Move 1"/>
            <Label fx:id="natureMove2Label" prefHeight="17.0" prefWidth="211.0" text="Move 2"/>
            <Label fx:id="natureMove3Label" prefHeight="17.0" prefWidth="233.0" text="Move 3"/>
            <Label fx:id="natureMove4Label" prefHeight="17.0" prefWidth="267.0" text="Move 4"/>
         </VBox>
      </VBox>
   </HBox>
</AnchorPane>
